{
  "name": "@consensusnetworks/parma",
  "version": "1.0.0",
  "main": "build/cjs/crawler.js",
  "exports": {
    "import": "./build/esm/crawler.js",
    "require": "./build/cjs/crawler.js"
  },
  "bin": {
    "parma": "build/cjs/cli.js"
  },
  "scripts": {
    "test": "tap --no-coverage --reporter=specy -J",
    "build:cjs": "esbuild crawler.ts cli.ts aws/** services/** --platform=node --target=node16 --format=cjs --outdir=./build/cjs",
    "build:esm": "esbuild crawler.ts cli.ts aws/** services/** --platform=node --target=node16 --format=esm --outdir=./build/esm",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "author": "hawyar",
  "dependencies": {
    "@aws-sdk/client-athena": "^3.121.0",
    "@aws-sdk/client-glue": "^3.121.0",
    "@aws-sdk/client-s3": "^3.120.0",
    "@aws-sdk/lib-storage": "^3.121.0",
    "@helium/crypto": "^4.3.1",
    "@helium/http": "^4.4.0",
    "@helium/transactions": "^4.3.1",
    "@iotexproject/iotex-address-ts": "^1.0.4",
    "accumulate.js": "^0.4.0",
    "arg": "^5.0.2",
    "esbuild": "^0.14.47",
    "iotex-antenna": "^0.30.10",
    "json-bigint": "^1.0.0",
    "nanospinner": "^1.1.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/json-bigint": "^1.0.1",
    "ts-standard": "^11.0.0",
    "tsx": "^3.6.0"
  },
  "ts-standard": {
    "ignore": [
      "bin",
      "dist"
    ]
  }
}
