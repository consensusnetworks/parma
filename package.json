{
  "name": "@consensusnetworks/parma",
  "version": "1.0.0",
  "main": "dist/crawler.cjs",
  "exports": {
    "import": "./dist/crawler.mjs",
    "require": "./dist/crawler.cjs"
  },
  "bin": {
    "parma": "dist/cli.cjs"
  },
  "scripts": {
    "test": "tap --no-coverage --reporter=specy -J",
    "build:cjs": "esbuild ./lib/Crawler.ts --minify --platform=node --target=node16 --format=cjs --outfile=./dist/crawler.cjs",
    "build:services": "esbuild ./lib/services/** --minify --platform=node --target=node16 --format=cjs --outdir=./dist/services",
    "build:esm": "esbuild ./lib/Crawler.ts --minify --platform=node --target=node16 --format=esm --outfile=./dist/crawler.mjs",
    "build:cli": "esbuild ./lib/Cli.ts --minify --platform=node --target=node16 --format=cjs --outfile=./dist/cli.cjs",
    "build": "rm -rf ./dist && npm run build:cjs && npm run build:esm && npm run build:cli && npm run build:services"
  },
  "author": "hawyar",
  "dependencies": {
    "@aws-sdk/client-athena": "^3.121.0",
    "@aws-sdk/client-glue": "^3.121.0",
    "@aws-sdk/client-s3": "^3.120.0",
    "@aws-sdk/lib-storage": "^3.121.0",
    "@helium/crypto": "^4.3.1",
    "@helium/http": "^4.4.0",
    "@helium/transactions": "^4.3.1",
    "@iotexproject/iotex-address-ts": "^1.0.4",
    "accumulate.js": "^0.4.0",
    "arg": "^5.0.2",
    "esbuild": "^0.14.47",
    "iotex-antenna": "^0.30.10",
    "json-bigint": "^1.0.0",
    "nanospinner": "^1.1.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/json-bigint": "^1.0.1",
    "ts-standard": "^11.0.0",
    "tsx": "^3.6.0"
  },
  "ts-standard": {
    "ignore": [
      "bin",
      "dist"
    ]
  }
}
